set +h
umask 022
LC_ALL=POSIX
PLX_TGT=$(uname -m)-plx-linux-gnu
PLX_TGT32=i686-plx-linux-gnu
PLX_TGTX32=x86_64-plx-linux-gnux32
PATH=/usr/bin
if [ ! -L /bin ]; then PATH=/bin:$PATH; fi
PATH=$PLX/tools/bin:$PATH
CONFIG_SITE=$PLX/usr/share/config.site
MAKEFLAGS=-j$(nproc)
export LC_ALL PLX_TGT PLX_TGT32 PLX_TGTX32 PATH CONFIG_SITE MAKEFLAGS
